func fib(until: int): int{
	if(until < 0){
		printStr("Incorrect input!")
		return 0
	}
	else if(until == 0){
		return 0
	}
	else if(until == 1){
		return 1
	}
	else{
		var fibNum: int = fib(until - 2) + fib(until - 1)
		return fibNum
	}
	return 0
}

func add(a: int, b: int, c: int): int{
	return a + b + c
}

func add(a: int, b: int): int{
	return a + b
}

func main(): int{
	printInt(fib(4))
	printInt(fib(5))
	printInt(fib(6))

	printInt(add(1,2,3))
	printInt(add(1,2))

	return 0
}